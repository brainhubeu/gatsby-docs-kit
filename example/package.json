{
  "name": "starter",
  "description": "Gatsby-Docs-Kit starter repository",
  "version": "0.0.21",
  "author": "Brainhub",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && rm -rf public && gatsby build --prefix-paths",
    "deploy:gh": "bash ../tools/deploy-github.sh",
    "develop": "gatsby clean && gatsby develop",
    "seed": "bash ../tools/seed.sh",
    "serve": "gatsby serve",
    "lint": "eslint src",
    "lint:autofix": "eslint --ext .jsx,.js . --fix"
  },
  "dependencies": {
    "@brainhubeu/gatsby-docs-kit": "^3.2.8",
    "gatsby": "^5.13.7",
    "gatsby-link": "^5.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^9.12.0",
    "eslint-config-brainhub": "^1.13.0",
    "gh-pages": "^6.2.0"
  }
}
