{
  "name": "@brainhubeu/gatsby-docs-kit-start",
  "description": "Gatsby-Docs-Kit starter repository",
  "version": "0.0.10",
  "author": "Brainhub",
  "license": "MIT",
  "scripts": {
    "build": "gatsbby clean && rm -rf public && gatsby build --prefix-paths",
    "deploy:gh": "bash ../tools/deploy-github.sh",
