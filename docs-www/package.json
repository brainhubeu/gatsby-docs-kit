    "gatsby-link": "^2.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-brainhub": "^1.12.0",
    "gh-pages": "^2.2.0"
  }
}
